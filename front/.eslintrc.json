{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "ignorePatterns": ["migrations/**"],
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "no-await-in-loop": "off",
    "import/prefer-default-export": "off",
    "react/state-in-constructor": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-alert": "off"
  }
}
